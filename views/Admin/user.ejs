<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>FreshFood</title>
    <script src="../../js/jquery.min.js"></script>
    <script src="../../js/jquery.validate.js"></script>
    <script src="../../js/bootstrap.min.js"></script>
    <link rel="shortcut icon" href="../../image/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="../../css/bootstrap.min.css" type="text/css" />
    <link rel="stylesheet" href="../../css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="../../css/admin.css" type="text/css" />


</head>

<body>



    <div class="container-fluid">

        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 text-center header">
                <a href="/">
                    <p>
                        <span style="color:rgb(27, 177, 40)">Fresh</span>
                        <i>food</i>
                    </p>
                </a>
            </div>
        </div>

        <div class="row" id="menu">
            <div id="thanhngang"></div>
            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 task text-center user">
                <a href="/user">
                    <i class="glyphicon glyphicon-user">
                        <span>Users</span>
                    </i>
                </a>
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 task text-center .product">
                <a href="/admin">
                    <i class="glyphicon glyphicon-tasks">
                        <span>Products</span>
                    </i>
                </a>
            </div>
            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4 task text-center .bill">
                <a href="/bill">
                    <i class="glyphicon glyphicon-print">
                        <span>Bill</span>
                    </i>
                </a>
            </div>
        </div>

        <div class="row tasks">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <a href="/them" class="btn" id="themsanpham">
                    Thêm Quản Lý Admin
                </a>
                <!--table admin-->
                
                <div class="panel panel-warning">
                      <div class="panel-heading">
                            <h3 class="panel-title">Quản Lý Admin</h3>
                      </div>
                      <div class="panel-body">
                            <table class="table table-bordered table-hover">
                                    <thead>
                                        <tr>
                                            <th>STT</th>
                                            <th>Tên</th>
                                            <th style="width: 100px;">Pass</th>
                                            <th style="width: 100px;">Email</th>
                                            <th style="width: 200px;">Hành Động</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td></td>
                                            <td>
                                                <form action="/timsanpham" method="POST">
                                                    <div class="input-group">
                                                        <input type="text" name="key" class="form-control" placeholder="Search products name...">
                                                        <span class="input-group-btn">
                                                            <button class="btn btn-primary" type="submit">
                                                                <i class="glyphicon glyphicon-search">&nbsp;Search</i>
                                                            </button>
                                                        </span>
                                                    </div>
                                                </form>
                                            </td>
                                            <td></td>
                                            <td colspan="4">
                                                <div class="dropdown">
                                                    <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Sap Xep
                                                        <span class="caret"></span>
                                                    </button>
                                                    <ul class="dropdown-menu">
                                                        <li>
                                                            <a href="/xepaz">
                                                                <span class="glyphicon glyphicon-sort-by-alphabet">&nbsp;Tu A->Z
                                                                </span>
                                                            </a>
                                                        </li>
                                                        <li>
                                                            <a href="/xepza">
                                                                <span class="glyphicon glyphicon-sort-by-alphabet-alt">&nbsp;Tu Z->A
                                                                </span>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </td>
                                        </tr>
                                        <% data.Items.forEach((data,index)=> { %>
                                            <%if(data.lv==2){%>
                                                <tr>
                                                    <td>
                                                        <%= index%>
                                                    </td>
                                                    <td>
                
                                                        <span>
                                                            <%= data.ten%>
                                                        </span>
                
                                                    </td>
                                                    <td>
                                                        <%= data.pass%>
                                                    </td>
                                                    <td>
                                                        <%= data.email%>
                
                                                    </td>
                
                                                    <td style="width:300px">
                                                        <a href="/suaSP/<%= data.Idprod%>/<%= data.IdprodC%>">
                                                            <label class="btn btn-danger">Sữa</label>
                                                        </a>
                
                                                        &nbsp; &nbsp;
                                                        <a href="/xoaSP/<%= data.Idprod%>/<%= data.IdprodC%>">
                                                            <label class="btn btn-warning">Xóa</label>
                                                        </a>
                
                
                                                    </td>
                                                </tr>
                                                <%}%>
                
                                                    <% }); %>
                                    </tbody>
                                </table>
                      </div>
                </div>
                
                <!--table admin-->
                <!--table user-->

                <div class="panel panel-warning" style="margin-top:40px">
                    <div class="panel-heading">
                        <h3 class="panel-title">Quản Lý Người Dùng</h3>
                    </div>
                    <div class="panel-body">
                        <table class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>STT</th>
                                    <th>Tên</th>
                                    <th style="width: 100px;">Pass</th>
                                    <th style="width: 100px;">Email</th>
                                    <th style="width: 200px;">Hành Động</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td></td>
                                    <td>
                                        <form action="/timsanpham" method="POST">
                                            <div class="input-group">
                                                <input type="text" name="key" class="form-control" placeholder="Search products name...">
                                                <span class="input-group-btn">
                                                    <button class="btn btn-primary" type="submit">
                                                        <i class="glyphicon glyphicon-search">&nbsp;Search</i>
                                                    </button>
                                                </span>
                                            </div>
                                        </form>
                                    </td>
                                    <td></td>
                                    <td colspan="4">
                                        <div class="dropdown">
                                            <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">Sap Xep
                                                <span class="caret"></span>
                                            </button>
                                            <ul class="dropdown-menu">
                                                <li>
                                                    <a href="/xepaz">
                                                        <span class="glyphicon glyphicon-sort-by-alphabet">&nbsp;Tu A->Z
                                                        </span>
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="/xepza">
                                                        <span class="glyphicon glyphicon-sort-by-alphabet-alt">&nbsp;Tu Z->A
                                                        </span>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </td>
                                </tr>
                                <% data.Items.forEach((data,index)=> { %>
                                    <%if(data.lv==1){%>
                                        <tr>
                                            <td>
                                                <%= index%>
                                            </td>
                                            <td>

                                                <span>
                                                    <%= data.ten%>
                                                </span>

                                            </td>
                                            <td>
                                                <%= data.pass%>
                                            </td>
                                            <td>
                                                <%= data.email%>

                                            </td>

                                            <td style="width:300px">
                                                <a href="/suaSP/<%= data.Idprod%>/<%= data.IdprodC%>">
                                                    <label class="btn btn-danger">Sữa</label>
                                                </a>

                                                &nbsp; &nbsp;
                                                <a href="/xoaSP/<%= data.Idprod%>/<%= data.IdprodC%>">
                                                    <label class="btn btn-warning">Xóa</label>
                                                </a>


                                            </td>
                                        </tr>
                                        <%}%>

                                            <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>

                <!--tabl user-->
            </div>
        </div>


    </div>









</body>

</html>